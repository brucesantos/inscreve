<!doctype html>
<html lang="pt">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Sistema Inscreve da FGV Conhecimento, para inscrições em concursos">
    <title>Inscreve - FGV Conhecimento</title>
    <link href="styles/main.css" rel="stylesheet">
  </head>

  <body class="bg-light">

    <main class="container">

      <section class="h-100">
        <div class="container h-100">
          <div class="row justify-content-sm-center h-100">
            <div class="col-lg-6 col-md-7 col-sm-9">
              <div class="text-center my-5">
                <img src="images/logo-fgv-conhecimento.svg" alt="logo">
              </div>
              <div class="card shadow-lg">
                <div class="card-body p-4">
                  <h1 class="fs-4 card-title fw-bold mb-4">Acesso (Login)</h1>
                  <form method="POST" class="needs-validation" novalidate="" autocomplete="off">
    
                    <!-- @import "cpf" -->

                    <!-- @import "senha" -->
    
                    <div class="d-flex align-items-center mt-4">
                      <div class="form-check">
                        <input type="checkbox" name="remember" id="remember" class="form-check-input">
                        <label for="remember" class="form-check-label">Lembre de mim</label>
                      </div>
                      <button type="submit" class="btn btn-primary ms-auto">
                        Entrar
                      </button>
                    </div>
                  </form>
                </div>
                <div class="card-footer py-3 border-0">
                  <div class="text-center">
                    Não tem uma Conta? <a href="register.html" class="text-dark">Cadastre-se</a>
                  </div>
                </div>
              </div>
              <!-- @import "versao" -->
            </div>
          </div>
        </div>
      </section>
      
    </main>

    <!-- scripts -->
    <script src="js/bootstrap.js"></script>

  </body>
</html>
