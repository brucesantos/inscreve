                        <li class="list-group-item">
                            <input class="form-check-input me-1" type="checkbox" value="tecnico-informatica" id="cargo19">
                            <label class="form-check-label stretched-link" for="cargo19">Técnico em Informática</label>
                        </li>
                        <li class="list-group-item">
                            <input class="form-check-input me-1" type="checkbox" value="especialista-geoinformacao" id="cargo20">
                            <label class="form-check-label stretched-link" for="cargo20">Especialista em Geoinformação</label>
                        </li>
                    </ul>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cancelar</button>
                    <button type="button" class="btn btn-primary">Salvar</button>
                </div>
            </div>
        </div>
    </div>

    <!-- scripts -->
    <script src="js/bootstrap.js"></script>
   
    <script>
        // inicializar o script e esconder os elementos necessários
        window.addEventListener('DOMContentLoaded', function () {
            document.getElementById('previewDados').setAttribute('hidden', 'true');
            document.getElementById('etapasDoConcurso').setAttribute('hidden', 'true');
            document.getElementById('respostaUmFeedback').setAttribute('hidden', 'true');
            document.getElementById('rodapeEnter').setAttribute('hidden', 'true');
            document.getElementById('cargosSelecionados').setAttribute('hidden', 'true');
            
            var perguntaUmRespostaSim = document.getElementById('perguntaUmRespostaSim');
            perguntaUmRespostaSim.addEventListener('change', function() {
                if (this.checked) {
                    document.getElementById('respostaUmFeedback').removeAttribute('hidden');
                }
            });

            var perguntaUmRespostaNao = document.getElementById('perguntaUmRespostaNao');
            perguntaUmRespostaNao.addEventListener('change', function() {
                if (this.checked) {
                    document.getElementById('respostaUmFeedback').setAttribute('hidden', 'true');
                }
            });

            // Estado dos cargos selecionados
            let cargosSelecionadosArr = [];

            // Função para renderizar a lista de cargos
            function renderizarListaCargos() {
                const listaCargos = document.getElementById('listaCargosSelecionados');
                const containerCargos = document.getElementById('cargosSelecionados');
                const alertaCargos = document.getElementById('alertaCargos');
                const ordemInstrucao = document.getElementById('ordemPreferenciaInstrucao');
                listaCargos.innerHTML = '';

                if (cargosSelecionadosArr.length > 0) {
                    // Troca o alerta para o texto de ordem de preferência
                    alertaCargos.innerHTML = '<strong>Atenção:</strong> Esta lista será utilizada para ranquear quais cargos sua pontuação atingiu, por ordem de preferência, de forma que, se você for aprovado para um determinado cargo, os demais cargos menos preferidos que ele serão desconsiderados.';
                    // Mostra instrução de ordem de preferência
                    ordemInstrucao.style.display = '';
                    cargosSelecionadosArr.forEach((cargo, idx) => {
                        const li = document.createElement('li');
                        li.className = 'list-group-item p-0 border-0';
                        li.innerHTML = `
                        <div class="px-2 mb-2 bg-light border rounded">
                            <div class="d-flex align-items-center justify-content-between gap-2">
                                <div class="text-center">
                                    <button class="btn btn-light btn-sm" style="width: 42px;" onclick="moverCargo(${idx}, -1)" ${idx === 0 ? 'disabled' : ''} title="Subir">
                                        <span class="text-primary">&#9650;</span>
                                    </button>
                                    <p class="fw-bold lh-1 m-0">${idx + 1}º</p>
                                    <button class="btn btn-light btn-sm" style="width: 42px;" onclick="moverCargo(${idx}, 1)" ${idx === cargosSelecionadosArr.length - 1 ? 'disabled' : ''} title="Descer">
                                        <span class="text-primary">&#9660;</span>
                                    </button>
                                </div>
                                <div class="fw-semibold flex-grow-1" style="font-size: 1.1em;">
                                    ${cargo.nome}
                                </div>
                                <button class="btn btn-sm btn-light" style="width: 42px;" onclick="toggleExpandirCargo(${idx})" title="${cargo.expanded ? 'Fechar' : 'Expandir'}">
                                    <span style="font-size: 1.5em; color: #757575;">${cargo.expanded ? '&times;' : '+'}</span>
                                </button>
                            </div>
                            <div class="cargo-detalhes border-top p-2 fs-sm" style="display: ${cargo.expanded ? 'block' : 'none'};">
                                <div class="mb-2">
                                    <p class="fw-bold mb-0">Especialidade:</p>
                                    <p class="mb-0">Análise e Desenvolvimento de Sistemas</p>
                                </div>
                                <div class="mb-2">
                                    <p class="fw-bold mb-0">Órgão:</p>
                                    <p class="mb-0">AGU</p>
                                </div>
                                <div class="mb-2">
                                    <p class="fw-bold mb-0">Vagas imediatas:</p>
                                    <p class="mb-0">AC: 2 &nbsp; PCD: 0 &nbsp; Negra: 1 &nbsp; Indígena: 0</p>
                                </div>
                                <div class="mb-2">
                                    <p class="fw-bold mb-0">Formação exigida:</p>
                                    <p class="mb-0">Lorem ipsum dolor sit amet, consectetur adipiscing elit. Curabitur suscipit metus lectus, non ultrices augue fermentum sit amet. Vestibulum eget.</p>
                                </div>
                                <button type="button" class="btn btn-outline-danger mb-2" onclick="removerCargo(${idx})">Remover</button>
                            </div>
                        </div>
                        `;
                        listaCargos.appendChild(li);
                    });
                    containerCargos.removeAttribute('hidden');
                } else {
                    // Volta o alerta para o texto original
                    alertaCargos.innerHTML = '<strong>Atenção:</strong> Você pode selecionar vários cargos e/ou especialidades de seu interesse, sem limite predefinido. Após a seleção, será necessário elencar os cargos em ordem de prioridade, conforme suas preferências.';
                    // Esconde instrução de ordem de preferência
                    ordemInstrucao.style.display = 'none';
                    containerCargos.setAttribute('hidden', 'true');
                }
            }

            // Função para mover cargo
            window.moverCargo = function(idx, direcao) {
                const novoIdx = idx + direcao;
                if (novoIdx < 0 || novoIdx >= cargosSelecionadosArr.length) return;
                const temp = cargosSelecionadosArr[idx];
                cargosSelecionadosArr[idx] = cargosSelecionadosArr[novoIdx];
                cargosSelecionadosArr[novoIdx] = temp;
                renderizarListaCargos();
            }

            // Função para expandir/colapsar cargo
            window.toggleExpandirCargo = function(idx) {
                cargosSelecionadosArr[idx].expanded = !cargosSelecionadosArr[idx].expanded;
                renderizarListaCargos();
            }

            // Função para remover cargo
            window.removerCargo = function(idx) {
                // Desmarcar o checkbox correspondente
                const cargoId = cargosSelecionadosArr[idx].id;
                const checkbox = document.getElementById(cargoId);
                if (checkbox) checkbox.checked = false;
                cargosSelecionadosArr.splice(idx, 1);
                renderizarListaCargos();
            }

            // Atualizar a lista de cargos selecionados ao salvar no modal
            document.querySelector('#modalCargos .btn-primary').addEventListener('click', function() {
                const checkboxes = document.querySelectorAll('#modalCargos input[type="checkbox"]:checked');
                // Atualizar cargosSelecionadosArr mantendo o estado de expansão se possível
                const novosCargos = Array.from(checkboxes).map(checkbox => {
                    const existente = cargosSelecionadosArr.find(c => c.id === checkbox.id);
                    return {
                        id: checkbox.id,
                        nome: checkbox.nextElementSibling.textContent,
                        expanded: existente ? existente.expanded : false
                    };
                });
                cargosSelecionadosArr = novosCargos;
                renderizarListaCargos();
                // Fechar o modal
                const modal = bootstrap.Modal.getInstance(document.getElementById('modalCargos'));
                modal.hide();
            });
        });

        // mostrar os detalhes do concurso após a escolha do cargo e esconder os calls to action para escolher o cargo
        document.getElementById('cargo').addEventListener('change', function () {
            document.getElementById('escolhaOCargo').setAttribute('hidden', 'true');
            document.getElementById('previewDados').removeAttribute('hidden');
            document.getElementById('etapasDoConcurso').removeAttribute('hidden');
            document.getElementById('rodapeEnter').removeAttribute('hidden');
        });
    </script>

    <script>
        Dropzone.options.myAwesomeDropzone = {
            paramName: "file", // The name that will be used to transfer the file
            maxFilesize: 8, // MB
            addRemoveLinks: true, // Enables the removal of file previews
            dictRemoveFile: '<img src="images/times.svg" alt="botão para remover o arquivo">', // botao de remover
            dictDefaultMessage: "Arraste os arquivos aqui para fazer upload, ou clique aqui para escolher" // mensagem em portugues, quando está vazio
        };
    </script>

    

  </body>
</html>
